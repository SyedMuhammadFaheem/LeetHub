{
    "manifest_version": 2,
    "name": "LeetHub",
    "version": "0.0.1.6",
    "description": "Commit LeetCode solutions to GitHub",
    "browser_action": {
      "default_icon": {
        "128": "icons/leetcode.png"
      },
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "icons": {
      "128": "icons/leetcode.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.leetcode.com/submissions"],
        "js": ["content.js"]
      }
    ]
  }
  